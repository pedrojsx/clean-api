{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch dev via NPM",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "dev"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node",
      "outFiles": [
        "${workspaceFolder}/**/*.ts",
        "!**/node_modules/**"
      ]
    }
  ]
}